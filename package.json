{
  "name": "@antv/insight-component",
  "version": "0.0.7",
  "description": "Components for GI & LI",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/insight-component.min.js",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "dumi dev",
    "lint": "run-p lint:*",
    "lint:ts": "eslint src __tests__ --ext .ts,.tsx",
    "lint:style": "stylelint --fix src/**/*.less",
    "prettier": "prettier --write 'src/*.{tsx,ts,md,json}'",
    "lint-fix": "eslint src __tests__ --fix --ext .ts,.tsx --format=pretty && npm run lint:style",
    "test": "jest",
    "test-cover": "jest --coverage",
    "clean": "rimraf dist",
    "size": "limit-size",
    "build": "father build",
    "doctor": "father doctor",
    "docs:build": "dumi build",
    "docs:build-analyze": "ANALYZE=1 dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "ci": "run-s lint doctor build size",
    "prepublishOnly": "npm run ci",
    "version": "node scripts/sync-version"
  },
  "dependencies": {
    "@antv/event-emitter": "^0.1.3",
    "@formily/antd": "^2.1.11",
    "@formily/core": "^2.1.11",
    "@formily/react": "^2.1.11",
    "ahooks": "^3.3.13",
    "classnames": "^2.3.1",
    "colorbrewer": "^1.5.3",
    "lodash-es": "^4.17.21",
    "react-color": "^2.19.3",
    "react-beautiful-dnd": "^13.1.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/dumi-theme-antv": "^0.2.2",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.6",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@umijs/fabric": "^3.0.0",
    "antd": "^4.22.3",
    "dumi": "^1.1.40",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "father": "^4.0.7",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-less-loader": "^0.1.2",
    "limit-size": "^0.1.4",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.4",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.22.3",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "*.less": "stylelint --fix",
    "*.{ts,tsx}": "eslint --fix",
    "*.{ts,tsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "limit-size": [
    {
      "path": "dist/insight-component.min.js",
      "limit": "200 Kb"
    }
  ],
  "keywords": [
    "antv",
    "insight",
    "component",
    "GI",
    "LI"
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "homepage": "https://insight-component.antv.vision",
  "repository": {
    "type": "git",
    "branch": "master",
    "url": "https://github.com/antvis/insight-component"
  },
  "bugs": {
    "url": "https://github.com/antvis/insight-component/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
